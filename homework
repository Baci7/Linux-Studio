(1)   
简单   1342    将数字变成0的操作次数
#include<stdio.h>
int main()
{
    int x,cnt=0;
    scanf("%d",&x);
    while(x>0){
        if(x%2==0){
            x=x/2;
            cnt++;
        }
        
        if(x%2!=0){
            x=x-1;
            cnt++;
        }
   }
    printf("%d",cnt);
    return 0;
}



(2)  
简单  面试题17  打印从1到最大的n位数
#include<stdio.h>
int main()
{
    int x=0,y,i,n;
    scanf("%d",&n);
    for(y=1;x<n;x++){
        y*=10;
    }
    for(i=1;i<y;i++){
        printf("%d ",i);
    }
    return 0;
}


(3)
简单  1281  整数的各位积和之差
#include<stdio.h>
int main()
{
    int x,y,i,add=0,mul=1,sum;
    scanf("%d",&x);
    for(i=0;x>0;i++){
        y=x%10;
        x/=10;
        add+=y;
        mul*=y;
    }
    sum=mul-add;
    printf("%d",sum);
    return 0;
}


(4)
简单  771     宝石与石头
#include<stdio.h>
#include<string.h>
int main()
{   int m,n,N,M,cnt=0;
    char J[50],S[50];
    scanf("%s %s",J,S);
    M=strlen(J);
    N=strlen(S);
    for(m=0;m<M;m++){
        for(n=0;n<N;n++){
            if(J[m]==S[n]){
                cnt++;
            }
        }

    }
    printf("%d",cnt);
    return 0;
}



(5)
简单  1025   除数博弈
#include<stdio.h>
int main()
{
    /*如果先手要赢最后得到的数一定是2，若要得到偶数则上一步一定是奇数相减，
    则对方上一次得到的一定是奇数，而偶数减奇数一定是奇数，所以要保证先手得到的数是偶数，
    对方得到的数是奇数，就能赢*/
    int n;
    printf("爱丽丝得到的第一个数：");
    scanf("%d",&n);
    if(n%2==0){
        printf("ture");
    }
    else{
        printf("false");
    }

}



(6)
简单  7   整数反转
#include<stdio.h>
int fz(int x);
int main()
{
    int x,y,k;
    scanf("%d",&x);
    k=x;
    if(k<0){
        x=-x;
    }
    y=fz(x);
    if(k<0){
     y=-y;
     }
    printf("%d",y);
return 0;
}

int fz(int x){
    int digit,ret=0;
    while(x>0){
        digit=x%10;
        ret=ret*10+digit;
        x/=10;}
    return ret;
}



(7)
简单  9  回文数
#include<stdio.h>
int main()
{
    int number,now=0,original,remainder;
    scanf("%d",&number);
    original=number;
    if(number<0){
        printf("false\n");
    }
    if(number>=0){
        while(number!=0){
            remainder=number%10;
            now=now*10+remainder;
            number/=10;
        }
        if(original==now){
            printf("ture\n");
        }
        else{
            printf("false\n");
        }
    }
    return 0;
}



(8) 简单  258 各位相加
#include<stdio.h>
int xx(int a,int b,int c);
int main()
{
    int i,n=0,m,sum;
    scanf("%d",&i);
    sum=xx(m,i,n);
    while(sum>=10){
       sum=xx(m,sum,n);
    }
    printf("%d",sum);
    return 0;
}


int xx(int a,int b,int c)
{
    for(a=0;b>=10;){
            a=b/10;
            b%=10;
         c+=a;
        }
        c+=b;
        return c;
}

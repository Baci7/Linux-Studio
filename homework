1 简单 整数反转
#include<stdio.h>
int fzs(int x);
int main()
{
	int x,y,k;
	scanf("%d",&x);
	k=x;
	if(k<0){
		x=-x;
	}
	y=fzs(x);
	if(k<0){
		y=-y;
	}
	printf("%d",y);
	return 0;
 }
 
 int fzs(int x){
 	int digit,ret=0;
 	while(x>0){
 		digit=x%10;
 		ret=ret*10+digit;
 		x/=10;
	 }
	 return ret;
 } 




2  简单  回文数
#include<stdio.h>
int main()
{
	int number,now=0,original,remainder;
	scanf("%d",&number);
	original=number;
	while(number!=0)
	{
		remainder=number%10;
		now=now*10+remainder;
		number=number/10;
	}
	if(original==now)
	     printf("true\n");
	     else
	     printf("sorry\n");
	return 0;
 } 



3 简单 无重复字符串的最长字串
#include<stdio.h>
#include<string.h>
#define SIZE 10
int main(void)
{
	char a[SIZE]="abcabcbb";
	int b[SIZE];
	int i,j,k;
	for(i=0;a[i];i++){
		r(b[i]=1,j=i+1;a[j];j++,b[i]++){
			for(k=i;k<j;k++){
				if(a[k] == a[j]){
				}
			}
			if(k<j){
				break;
			}
		}
	}
	int max=b[0];
	for(i=0;i<strlen(a);i++){
		if(b[i>max])
		max=b[i];
	}
	printf("%d\n",max);
		return 0;
 } 



4  简单  外观数列
#include<stdio.h>
int main()
{
	int N,count;
	char string1[100000]={0},string2[100000]={0};
	char*s1=string1,*s2=string2,*temp;
	char*p1,*p2;
	scanf("%s %d",s1,&N);
	for(int i=1;i<N;i++)
	{
		for(p1=s1,p2=s2,count=0;*p1;p1++)
		{
			count++;
			if(*p1!=*(p1+1))
			{
				*p2++=*p1;
				*p2++=count+'0';
				count=0;
			}
		}
		temp=s1,s1=s2,s2=temp;
	}
	puts(s1);
	return 0;
}


5 简单  整数的各位积和之差
#include<stdio.h>
int main()
{
	int x,y,i,add=0,mul=1,sum;
	scanf("%d",&x);
	for(i=0;x>0;i++){
		y=x%10;
		xl=10;
		add+=y;
		mul*=y;
	}
	sum=mul-add;
	printf("%d",sum);
	return 0;
 } 



6 简单 打印从1到最大的时间n位数
#include<stdio.h>
int main()
{
	int x=0,y,i,n;
	scanf("%d",&n);
	for(y=1;x<n;x++){
		y*=10;
	}
	for(i=1;i<y;i++){
		printf("%d",i);
	}
  return 0;	
}


7 简单 7简单  第一行为N和m表达正数序列的长度和指定的数字 中间用一个空格分开
#include<stdio.h>
int main()
{
	int N,m,i,temp,num;
	scanf("%d %d",&N,&m);
	for(i=0,num=0;i<N;i++){
		scanf("%d",&temp);
		if(m==temp){
			num++;
		}
	}
	printf("%d",num);
	return 0;
}



8 简单 除数博弈
#include<stdio.h>
int main()
{
	int n;
	printf("第一个数:");
	scanf("%d",&n);
	if(n%2==0){
		printf("ture");
	}
	else{
		printf("false");
	}
	return 0;
}
